{
  "name": "My First",
  "description": "First Steps",
  "version": "0.1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "savePhrase.js"
  },
   "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "http://127.0.0.1:8000/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["tab.html", "styles.css", "tab.js"],
      "matches": ["<all_urls>"]
    }
  ]
}